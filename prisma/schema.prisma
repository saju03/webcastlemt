generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model UserPhone {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  phone     String
  createdAt DateTime @default(now())
}

model EventLog {
  id        Int      @id @default(autoincrement())
  userEmail String
  eventName String
  eventTime DateTime
  called    Boolean  @default(false)
  createdAt DateTime @default(now())
}